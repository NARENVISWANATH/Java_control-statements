package javaapplication6;

/**
 *
 * @author NAREN
 */
public class Controlstatements3 {
    public static void main(String args[])
    {
        int num1 = 150;
        int num2 = 18;
        
        if(num1>num2)//condition
        {
            System.out.println("Number1 is greater: "+num1);
        }
        else
        {
            System.out.println("Number2 is greater: "+num2);
        }
    }
    
}
    

