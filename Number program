package javaapplication6;

/**
 *
 * @author NAREN
 */
public class Controlstatements1 {
    public static void main(String args[])
    {
        int num1=200;
        if(num1==20)//condition
        {
            System.out.println("Number is equal: "+num1);
        }
        else
        {
            System.out.println("Number is not equal");
        }
    }
    
}

    

