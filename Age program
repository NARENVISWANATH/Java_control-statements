package javaapplication6;

/**
 *
 * @author NAREN
 */
public class Controlstatements {
    public static void main(String args[])
    {
        int age=30;
        if(age<25)//condition
        {
            System.out.println("Age is lesser: +age");
        }
        else
        {
            System.out.println("Age is greater");
        }
    }
    
}
